{

  "manifest_version": 2,
  "name": "myPage",
  "version": "1.0",

  "description": "customize my own web page",

  "icons": {
    "48": "icons/page_icon.png"
  },

  "browser_action": {
    "default_icon": "icons/ext_logo.png",
    "default_title": "Set your tab timer",
    "default_popup": "popup/choose_mode.html" 
  },

  "background": {
    "scripts": ["src/background.js"]
  }, 

  "permissions": [
	  "storage",
	  "coockis",
	  "webNavigation",
	  "tabs",
	  "activeTab",
	  "<all_urls>"
  ]
}
